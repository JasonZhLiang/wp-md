<body  class="portal-exterior-body" style="overflow-x: hidden;">
    <div portalconfig></div>
    <div ng-controller="loginCtrl">
        <div id="topbar">
            <div class="row">
                <div class="col-md-12">

                    <img
                        id="login-brand-logo"
                        ng-src="{{ portalConfig.loginLogoUrl }}"
                        onerror="if (this.src != '../images/signup/LOGO_Login_OnBlue.png') this.src = '../images/signup/LOGO_Login_OnBlue.png';"
                        style="width: 250px; height: 80px;">

                    <div class="pull-right" ng-show="selfRegistrationEnabled" hiddenwhenbranded>

                        <span class="title" id="header-right-text" style="margin-right: 10px;margin-top: 5px;" translate>New User?</span>

                        <a id ="signup-btn"  href="{{ 'signup' | externallink }}" class="btn btn-info pull-right"  style="margin-right: 10px;" translate>SIGN UP</a>

                        <span class="title  pull-right" id="" style="margin-right: 10px;margin-top: 5px;" translate>OR</span>

                        <a id="learn-more-btn header-right"
                           href="http://www.getbraincloud.com" style="margin-right: 10px;padding: 7px;"
                           target="_blank" class="btn btn-yellow pull-right"  >
                            <b translate>LEARN MORE</b>
                        </a>

                    </div>
                </div>
            </div>
        </div>

        <div portalconfig></div>

        <div id="login-container">
            <div class="inner">
                <div id="banner-box" class="row">
                    <globalmessage />
                </div>

                <div class="center-block text text-center">
                    <h3 id="title-text" class="text-left text-yellow" translate>WELCOME BACK</h3>

                    
                </div>

                <div id="login-panel" class="form-group panel mt-15">
                    <form id="email-login" ng-show="showEmailPasswordLogin()" ng-submit="login()" name='f'>
                        <div class="center-block">
                            
                                
                                
                                    <input type="text"
                                        value="" ng-attr-placeholder="{{getLocalizedString('Email address')}}" name="j_username" id="j_username" ng-model="userCredentials.username" ng-disabled="waitingOnAuth"  class="form-control" />
                                
                            
                        </div>

                        <div class="center-block mt-15">
                            <input type="password" value="" ng-disabled="waitingOnAuth" ng-attr-placeholder="{{getLocalizedString('Password')}}" name='j_password' id="j_password" ng-model="userCredentials.password" class="form-control" />
                        </div>

                        <div class="center-block text text-center mt-15">
                            <button id="signin-btn" class="btn btn-info" ng-disabled="waitingOnAuth" style="width: 280px; font-size: 20px;" translate>LOG IN</button>
                        </div>

                        <div id="forgotpass-link-box">
                            <div class="center-block text text-center">
                                <a href="{{ 'forgot-password' | externallink }}" id="forgot-password-text" class="link-item" translate>Forgot Password?</a>
                            </div>
                        </div>
                    </form>

                    <div ng-show="!showEmailPasswordLogin() && showSSOLogin()" id="sso-lock-icon" class="center-block text-center">
                        <img src="../images/signup/BC-SSO-LockIcon.png" />
                    </div>

                    <div ng-show="showSSOLogin()" id="sso-login" style="display: flex; flex-direction: column;">
                        <hr style="margin-left: -15px; margin-right: -15px;" />
                        <button class="btn btn-info" style="width: 280px; font-size: 20px; margin-top: 15px;" ng-repeat="ssoLink in portalLoginConfig.ssoLinks" ng-click='ssoLogin(ssoLink.url);'>{{ ssoLink.label }}</button>
                    </div>
                </div>
            </div>
        </div> 
    
    <div id="versionNum" class="system-version" hiddenwhenbranded>
        <span class="platform-version">R4.13.1-7706</span>
        <br>
        <a ng-show="statusPageEnabled == 'true'" ng-href="{{statusPageUrl}}" target="_blank" class="status-link no-decorartion">
            <span  ng-show="showStatusIoState" id="status-icon">
                <img id="status-icon-img" ng-src="{{statusPageStatus.iconSrc}}" style="margin-bottom: 2px;" alt="bullet, red icon" width="16" height="16">
            </span>
            <span id="status-box" class="platform-version ml-5">{{statusPageStatus.message | translate}}</span>
        </a>
    </div>
</div>

