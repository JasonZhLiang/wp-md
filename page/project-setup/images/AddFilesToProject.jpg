        <script>
			// Pending activation - if no loggedIn cookie exists, we assume this a first time visitor so can redirect just once to sign up page
			if (navigator.cookieEnabled) {
				var value = "; " + document.cookie;
				var parts = value.split("; newArrival=");
				if (parts.length !== 2) {
					/// document.location = "signup";
				}

				// If we received a deep-link but spring kicked us out to the login page due to authentication,
				// a document frag will be left behind. We cache this frag in a short-expiry cookie
				// in order for the select_company.js page to redirect us properly once the
				// user has been logged in.
				var ndx = document.location.toString().lastIndexOf("#");
				if (ndx !== -1) {
					var fragment = document.location.toString().substring(ndx);
					var expiryDate = new Date((new Date()).getTime() + 1000 * 60 * 5); // 5 mins expiry
					document.cookie = "docfrag=" + fragment + ";expires=" + expiryDate.toUTCString() + ";path=/";
				} else {
					document.cookie = "docfrag=;expires=Thu, 01 Jan 1970 i00:00:01 GMT;";
				}
			}

        </script>
